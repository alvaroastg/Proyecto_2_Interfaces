<Page x:Class="bdInterfaces.Pages.Consultas"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:fo="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp"

      xmlns:local="clr-namespace:bdInterfaces.Pages"
      mc:Ignorable="d"
      Title="Consultas" Height="450" Width="600">
    <StackPanel>
        <StackPanel Orientation="Horizontal"
                    Margin="10,10,10,0">
            <fo:IconImage Icon="List"
                          Foreground="{StaticResource Verde}"
                          Margin="0,0,10,5"/>
            <TextBlock Text="CONSULTAS"
                       FontSize="20"
                       FontWeight="Bold"
                       FontFamily="MontSerrat"
                       Foreground="{StaticResource Verde}"></TextBlock>
        </StackPanel>
    </StackPanel>
</Page>
